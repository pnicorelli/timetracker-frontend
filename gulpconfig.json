{
  "jsSrc":[
    "app/app.js",
    "app/**/*module.js",
    "app/**/*route.js",
    "app/**/*.js",
    "!app/**/*specs.js"
  ],

  "htmlSrc":[
    "app/**/*.html"
  ],

  "cssSrc":[
    "app/**/*.css",
    "!app/shared/customCss/*.css"
  ],

  "jsonSrc":[
    "app/**/*.json"
  ],

  "buildLibs": [
    "build/libs",
    "build/view",
    "build/vendors"
  ],

  "vendorsJS": [
    "build/vendors/angular/angular.min.js",
    "build/vendors/angular-route/angular-route.min.js",
    "build/vendors/ngstorage/ngStorage.min.js",
    "build/vendors/angular-resource/angular-resource.min.js",
    "build/vendors/angular-aria/angular-aria.min.js",
    "build/vendors/angular-messages/angular-messages.min.js",
    "build/vendors/angular-material/angular-material.min.js",
    "build/vendors/jquery/dist/jquery.min.js",
    "build/vendors/bootstrap/dist/js/bootstrap.min.js",
    "build/vendors/angular-animate/angular-animate.min.js",
    "build/vendors/angular-notifier/dist/angular-notifier.min.js",
    "build/vendors/angular-smart-table/dist/smart-table.min.js",
    "build/vendors/angular-qrcode/angular-qrcode.js",
    "build/vendors/qrcode-generator/js/qrcode.js",
    "build/vendors/moment/min/moment-with-locales.min.js",
    "build/vendors/angular-datepicker/dist/angular-datepicker.min.js",
    "build/vendors/angular-sanitize/angular-sanitize.min.js",
    "build/vendors/ng-csv/build/ng-csv.min.js"
  ],

  "vendorsCSS": [
    "build/vendors/angular-material/angular-material.min.css",
    "build/vendors/bootstrap/dist/css/bootstrap.min.css",
    "build/vendors/bootstrap/dist/css/bootstrap-theme.min.css",
    "build/vendors/angular-notifier/dist/angular-notifier.min.css",
    "build/vendors/angular-datepicker/dist/angular-datepicker.min.css"
  ],

  "vendorsRequired": [
    "build/vendors/bootstrap/fon*/*.*"
  ],

  "DEST": "build/libs"
}
