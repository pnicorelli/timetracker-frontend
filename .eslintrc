{

  "env": {
    "browser": true,
    "node": true,
    "amd": true,
    "jasmine": true,
    "es6": true
  },

  "globals": {
    "angular": true
  },

  "rules": {
    "semi": 2,
    "comma-dangle": 1,
    "curly": 1,
    "strict": 1,
    "no-use-before-define": 0,
    "brace-style": 1,
    "comma-spacing": 1,
    "key-spacing": 1,
    "quotes": [2, "single"]
  }
}
